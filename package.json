  "mailgun-js": "^0.22.0",
  "mime": "^2.4.6",
  "mime-types": "^2.1.27",
  "moment": "^2.29.1",
  "mongoose": "^5.10.13",
  "mongoose-aggregate-paginate-v2": "^1.0.42",
  "mongoose-paginate-v2": "^1.3.16",
  "morgan": "^1.9.1",
  "multer": "^1.4.3",
  "node-cache": "^5.1.2",
  "node-cron": "^3.0.0",
  "passport": "^0.4.1",
  "passport-facebook": "^3.0.0",
  "passport-google-oauth2": "^0.2.0",
  "passport-jwt": "^4.0.0",
  "path": "^0.12.7",
