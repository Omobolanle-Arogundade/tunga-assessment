{
 "name": "banagos-api",
 "version": "0.0.1",
 "description": "A node-express API server for Test API v1",
 "main": "src/index.js",
 "author": "Omobolanle Aro <omobolanlearogundade@gmail.com>",
 "license": "MIT",
 "private": true,
 "scripts": {
  "start": "babel ./src/ -d ./dist && node dist/index.js",
  "start:prod": "babel ./src/ -d ./dist && node dist/index.js",
  "devstart": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
  "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
  "build": "babel ./src/ -d ./dist",
  "heroku-prebuild": "npm install",
  "test": "jest -i",
  "test:watch": "jest -i --watchAll",
  "coverage": "jest -i --coverage",
  "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
  "lint": "eslint .",
  "lint:fix": "eslint . --fix",
  "prettier": "prettier --check **/*.js",
  "prettier:fix": "prettier --write **/*.js",
  "docker:prod": "docker-compose -f ./setup/docker-compose.yml -f ./setup/production/docker-compose.yml up",
  "docker:dev": "docker-compose -f ./setup/docker-compose.yml -f ./setup/development/docker-compose.yml up",
  "docker:test": "docker-compose -f ./setup/docker-compose.yml -f ./setup/production/docker-compose.yml up",
  "seed": "cross-env NODE_ENV=development babel-node ./src/seeders",
  "docgen": "docgen build -i ./src/docs.json -o ./src/index.html"
 },
 "engines": {
  "node": ">=12.x",
  "npm": "6.x"
 },
 "dependencies": {
  "@babel/cli": "^7.10.1",
  "@babel/core": "^7.10.5",
  "@babel/node": "^7.12.6",
  "@babel/preset-env": "^7.10.2",
  "@babel/register": "^7.10.1",
  "@bugsnag/js": "^7.11.0",
  "@bugsnag/plugin-express": "^7.11.0",
  "@hapi/joi": "^17.1.0",
  "@supercharge/promise-pool": "^2.1.0",
  "axios": "^0.19.2",
  "babel-plugin-module-resolver": "^4.0.0",
  "bcryptjs": "^2.4.3",
  "body-parser": "^1.19.0",
  "cli-table": "^0.3.1",
  "cloudinary": "^1.25.0",
  "compression": "^1.7.4",
  "cors": "^2.8.5",
  "cross-env": "^7.0.0",
  "crypto": "^1.0.1",
  "dotenv": "^8.2.0",
  "express": "^4.17.1",
  "express-list-endpoints": "^5.0.0",
  "express-mongo-sanitize": "^2.0.0",
  "express-rate-limit": "^5.0.0",
  "faker": "^5.5.3",
  "helmet": "^3.21.2",
  "http-status": "^1.5.0",
  "https": "^1.0.0",
