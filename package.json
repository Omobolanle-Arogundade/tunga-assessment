  "babel-jest": "^26.6.3",
  "coveralls": "^3.0.7",
  "eslint": "^7.13.0",
  "eslint-config-airbnb-base": "^14.2.1",
  "eslint-config-prettier": "^6.15.0",
  "eslint-plugin-import": "^2.18.2",
  "eslint-plugin-jest": "^23.0.2",
  "eslint-plugin-prettier": "^3.1.1",
  "eslint-plugin-security": "^1.4.0",
  "jest": "^26.6.3",
  "lint-staged": "^10.5.1",
  "node-mocks-http": "^1.8.0",
